{

  "description": "Puts links to relevant news articles next to tweets",
  "manifest_version": 2,
  "name": "Autostories for Twitter",
  "version": "1.0",

  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/choose_page.html",
    "default_icon": {
      "16": "icons/page-16.png",
      "32": "icons/page-32.png"
    }
  }

}
